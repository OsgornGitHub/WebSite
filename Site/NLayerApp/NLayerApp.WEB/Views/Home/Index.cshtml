@model IEnumerable<NLayerApp.WEB.Models.ArtistViewModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="artist">
    @foreach (var person in Model)
    {
        <a href="http://localhost:54638/Artist/GetArtist?name=@person.Name">
            <div class="col-md-2">
                <img src="@person.Photo" style="width: 100%" />
                <h4 class="text-center">@person.Name</h4>

            </div>
        </a>

    }
</div>